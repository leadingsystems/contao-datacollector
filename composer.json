{
  "name": "leadingsystems/contao-datacollector",
  "description": "Leading Systems DataCollector as a Contao bundle",
  "keywords": [
    "contao",
    "leading systems",
    "bundle"
  ],
  "type": "contao-bundle",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "LeadingSystems\\DataCollectorBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "LeadingSystems\\DataCollectorBundle\\ContaoManager\\Plugin",
    "branch-alias": {
//      @toDo: change to 3.0.x-dev before release
      "dev-master": "2.1.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  }
}
